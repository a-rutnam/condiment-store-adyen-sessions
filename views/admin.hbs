<h1>Admin: Orders</h1>
<hr/>

<table class="u-full-width">
  <thead>
    <tr>
      <th>id</th>
      <th>user_id</th>
      <th>status</th>
      <th>adyen data</th>
    </tr>
  </thead>
  <tbody>
    {{#each orders}}
    <tr>
      <td>{{ this._id }}</td>
      <td>{{ this.user_id }}</td>
      <td>{{ this.status }}</td>
      <td>
        <details>
          <summary>Response and Request logs</summary>
          <br>
          <pre>
            {{ json this.adyenData }}
          </pre>
        </details>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
